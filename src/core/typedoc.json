{
    "entryPoints": ["./server/**/*.ts", "./shared/**/*.ts", "./client/**/*.ts"],
    "exclude": ["./server/api/**/*.ts", "./server/api/**/*.ts"],
    "out": "../../docs",
    "skipErrorChecking": true,
    "tsconfig": "./custom.json",
    "plugin": ["typedoc-theme-hierarchy", "typedoc-plugin-external-resolver", "typedoc-plugin-missing-exports"],
    "theme": "hierarchy",
    "basePath": "./",
    "entryPointStrategy": "expand",
    "excludeInternal": true,
    "excludeNotDocumented": true
}
